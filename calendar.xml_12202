<CREATETRIGGERS>
<CREATETRIGGER>
<ITEM DB="oracle">
CREATE OR REPLACE TRIGGER event_update
 AFTER UPDATE OF EVNGRP_ID, EvnFolder,EVN_ID ON Event FOR EACH ROW
BEGIN
    UPDATE EventMyMention SET  MEN_FOLDER = :new.EvnFolder, MENGRP_ID=:new.EVNGRP_ID, MENEVN_ID=:new.EVN_ID WHERE MENEVN_ID = :old.EVN_ID;
    UPDATE EventPin SET PIN_FOLDER = :new.EvnFolder, PINGRP_ID=:new.EVNGRP_ID, PINEVN_ID=:new.EVN_ID WHERE PINEVN_ID = :old.EVN_ID;
    UPDATE GlobalEventPin SET PIN_FOLDER = :new.EvnFolder, PINGRP_ID=:new.EVNGRP_ID, PINEVN_ID=:new.EVN_ID WHERE PINEVN_ID = :old.EVN_ID;
    UPDATE EventMyReaction SET REA_FOLDER = :new.EvnFolder, REAGRP_ID=:new.EVNGRP_ID, REAEVN_ID=:new.EVN_ID WHERE REAEVN_ID = :old.EVN_ID;
    UPDATE EventComment SET COM_FOLDER = :new.EvnFolder, COMGRP_ID=:new.EVNGRP_ID, COMEVN_ID=:new.EVN_ID WHERE COMEVN_ID = :old.EVN_ID;
    UPDATE OnlineDocumentEditing SET ODE_FOLDER = :new.EvnFolder, ODEGRP_ID=:new.EVNGRP_ID, ODEEVN_ID=:new.EVN_ID WHERE ODEVN_ID = :old.EVN_ID;
END;
</ITEM>
</CREATETRIGGER>
</CREATETRIGGERS>

