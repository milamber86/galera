<ALLEVENTNONRECURRENT>Select EVN_ID, EVNGRP_ID, EVNRCR_ID, EVNOWN_ID, EvnLockOWN_ID, EvnModifiedOWN_ID,
EvnShareType,
EvnStartDate,
EvnStartTime,
EvnEndDate,
EvnEndTime,
EvnTimeFormat,
EvnUID,
EvnFlags %s From Event MainEvent
LEFT OUTER JOIN EventReminder ON EventReminder.RMNEVN_ID = MainEvent.EVN_ID
Where (EVNGRP_ID = '%1:s') %4:s And ((EvnClass = 'E' And ((EVNRCR_ID Is Null) Or (EVNRCR_ID = ''))) Or (EvnClass = 'O') Or (EvnClass = 'V')) And
(
  (
    (EvnStartDate &gt;= %2:d) And (EvnStartDate &lt;= %3:d)
  )
  Or
  (
    (EvnStartDate &lt;= %2:d) And ((EvnEndDate &gt; %2:d) Or ((EvnEndDate = %2:d) And (EvnEndTime &gt; 0)))
  )
) ORDER BY EvnStartDate, EvnStartTime</ALLEVENTNONRECURRENT>

